    <view class="attr-pop">
      <view class="close" bindtap="closeQRCode">
        <image class="icon" src="/static/images/icon_close.png"></image>
      </view>
      <view class="img-info">
        <image class="img" src="{{userInfo.inviteUrl}}"></image>
      
